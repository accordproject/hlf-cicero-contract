{
    "name": "cicero-contract",
    "version": "0.61.8",
    "description": "Accord Project Cicero contracts on Hyperledger Fabric",
    "main": "index.js",
    "engines": {
        "node": ">=12",
        "npm": ">=5"
    },
    "scripts": {
        "lint": "eslint .",
        "pretest": "npm run lint",
        "test": "nyc mocha --recursive",
        "start": "fabric-chaincode-node start"
    },
    "engineStrict": true,
    "author": "Accord Project",
    "license": "Apache-2.0",
    "dependencies": {
        "@accordproject/cicero-core": "^0.21.26",
        "@accordproject/cicero-engine": "^0.21.26",
        "@accordproject/markdown-cicero": "^0.12.12",
        "bent": "^7.1.0",
        "fabric-contract-api": "^2.2.1",
        "fabric-shim": "^2.2.1"
    },
    "devDependencies": {
        "chai": "^4.2.0",
        "chai-as-promised": "^7.1.1",
        "eslint": "^5.9.0",
        "fabric-client": "^1.4.11",
        "mocha": "^5.2.0",
        "nyc": "^14.0.0",
        "sinon": "^7.1.1",
        "sinon-chai": "^3.3.0",
        "winston": "^3.2.1"
    },
    "nyc": {
        "exclude": [
            ".eslintrc.js",
            "coverage/**",
            "test/**"
        ],
        "reporter": [
            "text-summary",
            "html"
        ],
        "all": true,
        "check-coverage": true,
        "statements": 100,
        "branches": 100,
        "functions": 100,
        "lines": 100
    }
}
